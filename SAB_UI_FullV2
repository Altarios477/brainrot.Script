local p = game:GetService("Players").LocalPlayer local r = game:GetService("RunService") local u = game:GetService("UserInputService") local g = Instance.new("ScreenGui", p:WaitForChild("PlayerGui")) g.ResetOnSpawn = false local m = Instance.new("Frame", g) m.Size = UDim2.new(0, 270, 0, 300) m.Position = UDim2.new(0.5, -135, 0.4, 0) m.BackgroundColor3 = Color3.fromRGB(25, 25, 45) m.BorderSizePixel = 0 m.Active = true m.Draggable = true local tn = {"Main","Visual","Misc"} local tb,tbfr,c = {},{},nil for i,n in ipairs(tn) do local b = Instance.new("TextButton", m) b.Size = UDim2.new(0, 90, 0, 30) b.Position = UDim2.new(0, (i-1)90, 0, 0) b.Text = n b.BackgroundColor3 = Color3.fromRGB(40, 40, 80) b.TextColor3 = Color3.new(1,1,1) b.Font = Enum.Font.SourceSansBold b.TextScaled = true b.BorderSizePixel = 0 tb[n] = b local f = Instance.new("Frame", m) f.Position = UDim2.new(0, 0, 0, 30) f.Size = UDim2.new(1, 0, 1, -30) f.BackgroundTransparency = 1 f.Visible = false tbfr[n] = f b.MouseButton1Click:Connect(function() for k,v in pairs(tbfr) do v.Visible = (k==n) end c = n end) end tbfr.Main.Visible = true c = "Main" local function add(tab,t,fn) local f = tbfr[tab] if not f then return end local n = #f:GetChildren() local b = Instance.new("TextButton", f) b.Size = UDim2.new(1, -20, 0, 40) b.Position = UDim2.new(0, 10, 0, 10 + n45) b.BackgroundColor3 = Color3.fromRGB(50, 50, 90) b.TextColor3 = Color3.new(1,1,1) b.Font = Enum.Font.SourceSansBold b.TextScaled = true b.BorderSizePixel = 0 b.Text = t b.MouseButton1Click:Connect(fn) end

add("Main", "ðŸ”¼ Infinite Jump", function() _G.ij = true u.JumpRequest:Connect(function() if _G.ij and p.Character and p.Character:FindFirstChild("Humanoid") then p.Character.Humanoid:ChangeState("Jumping") end end) end)

add("Main", "ðŸšª Noclip", function() r.Stepped:Connect(function() if p.Character then for _,v in pairs(p.Character:GetDescendants()) do if v:IsA("BasePart") then v.CanCollide = false end end end end) end)

add("Main", "ðŸ’š Godmode", function() local c = p.Character if c and c:FindFirstChild("Humanoid") then c.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Dead, false) c.Humanoid.Health = math.huge end end)

add("Main", "ðŸš˜ Auto Farm", function() _G.farm = true while _G.farm and task.wait(0.2) do for _,v in pairs(workspace:GetDescendants()) do if v:IsA("BasePart") and v.Name:lower():find("coin") then if p.Character and p.Character:FindFirstChild("HumanoidRootPart") then p.Character.HumanoidRootPart.CFrame = v.CFrame + Vector3.new(0, 3, 0) end end end end end)

add("Main", "âœ¨ Auto Rebirth", function() _G.rebirth = true while _G.rebirth and wait(1) do local rmt = game:GetService("ReplicatedStorage"):FindFirstChild("Rebirth") if rmt then pcall(function() rmt:FireServer() end) end end end)

add("Main", "âš¡ Aura Hit", function() _G.aura = true while _G.aura and wait(0.3) do for _,v in pairs(game:GetService("Players"):GetPlayers()) do if v ~= p and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then if (p.Character.HumanoidRootPart.Position - v.Character.HumanoidRootPart.Position).Magnitude < 10 then local atk = game:GetService("ReplicatedStorage"):FindFirstChild("Damage") if atk then pcall(function() atk:FireServer(v.Character) end) end end end end end end)

add("Main", "ðŸ›‹ï¸ TP to Player", function() local target = nil local dist = math.huge for _,v in pairs(game:GetService("Players"):GetPlayers()) do if v ~= p and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then local d0 = (p.Character.HumanoidRootPart.Position - v.Character.HumanoidRootPart.Position).Magnitude if d0 < dist then dist = d0 target = v end end end if target then p.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(0,3,0) end end)

add("Visual", "ðŸ‘ ESP", function() for _,pl in pairs(game:GetService("Players"):GetPlayers()) do if pl ~= p then pl.CharacterAdded:Connect(function(c) local h = c:WaitForChild("Head", 5) if h and not h:FindFirstChild("ESP") then local b = Instance.new("BillboardGui", h) b.Name = "ESP" b.AlwaysOnTop = true b.Size = UDim2.new(0, 100, 0, 40) b.StudsOffset = Vector3.new(0, 2, 0) local t = Instance.new("TextLabel", b) t.Size = UDim2.new(1, 0, 1, 0) t.BackgroundTransparency = 1 t.TextColor3 = Color3.new(1, 0.4, 0.4) t.TextScaled = true t.Font = Enum.Font.SourceSansBold r.RenderStepped:Connect(function() if pl.Character and pl.Character:FindFirstChild("HumanoidRootPart") and p.Character then local d = (p.Character.HumanoidRootPart.Position - pl.Character.HumanoidRootPart.Position).Magnitude t.Text = pl.Name .. " [" .. math.floor(d) .. "Ð¼]" end end) end end) end end end)

add("Visual", "â± Ð¢Ð°Ð¹Ð¼ÐµÑ€Ñ‹", function() for _,o in ipairs(workspace:GetDescendants()) do if o:IsA("TextLabel") or o:IsA("TextBox") then if string.find(o.Text, "%d+s") then local p0 = o:FindFirstAncestorWhichIsA("BasePart") or o:FindFirstAncestorOfClass("Model") if p0 and not p0:FindFirstChild("TimerESP") then local g = Instance.new("BillboardGui", p0) g.Name = "TimerESP" g.AlwaysOnTop = true g.Size = UDim2.new(0, 120, 0, 30) g.StudsOffset = Vector3.new(0, 4, 0) local l = Instance.new("TextLabel", g) l.Size = UDim2.new(1, 0, 1, 0) l.BackgroundTransparency = 1 l.TextColor3 = Color3.new(0.4, 1, 0.4) l.TextScaled = true l.Font = Enum.Font.SourceSansBold r.RenderStepped:Connect(function() if o.Text and o:IsDescendantOf(workspace) then l.Text = o.Text end end) end end end end end)

add("Visual", "ðŸŽ¯ Player Tracker", function() end) add("Visual", "ðŸ”¦ Highlight Players", function() end)

add("Misc", "âš¡ SpeedBoost", function() p.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = 80 end)

add("Misc", "ðŸ’¤ Anti-AFK", function() local vu = game:GetService("VirtualUser") p.Idled:Connect(function() vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame) wait(1) vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame) end) end)

add("Misc", "ðŸ“ Ð¢ÐŸ Ð²Ð½Ð¸Ð¶", function() local r0 = p.Character and p.Character:FindFirstChild("HumanoidRootPart") if r0 then r0.CFrame = CFrame.new(r0.Position.X, 15, r0.Position.Z) end end)

add("Misc", "ðŸ—‘ Ð£Ð´Ð°Ð»Ð¸Ñ‚ÑŒ ÐÐ½Ñ‚Ð¸Ñ‡Ð¸Ñ‚", function() for _,o in ipairs(getnilinstances()) do if o:IsA("Script") and string.find(o.Name:lower(), "anti") then o:Destroy() end end end)
