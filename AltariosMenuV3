local p = game:GetService("Players").LocalPlayer local r = game:GetService("RunService") local u = game:GetService("UserInputService") local g = Instance.new("ScreenGui") g.Name = "SAB_UI" g.ResetOnSpawn = false g.Parent = p:WaitForChild("PlayerGui")

local m = Instance.new("Frame") m.Size = UDim2.new(0, 270, 0, 330) m.Position = UDim2.new(0.5, -135, 0.4, 0) m.BackgroundColor3 = Color3.fromRGB(25, 25, 45) m.BorderSizePixel = 0 m.Active = true m.Draggable = true m.Parent = g

local title = Instance.new("TextLabel") title.Size = UDim2.new(1, 0, 0, 30) title.Position = UDim2.new(0, 0, 0, 0) title.BackgroundColor3 = Color3.fromRGB(35, 35, 65) title.TextColor3 = Color3.new(1, 1, 1) title.Font = Enum.Font.SourceSansBold title.TextScaled = true title.Text = "Altarios_SAB" title.Parent = m

local tn = {"Main", "Visual", "Misc"} local tb, tbfr, c = {}, {}, nil

for i, n in ipairs(tn) do local b = Instance.new("TextButton") b.Size = UDim2.new(0, 90, 0, 30) b.Position = UDim2.new(0, (i - 1) * 90, 0, 30) b.Text = n b.BackgroundColor3 = Color3.fromRGB(40, 40, 80) b.TextColor3 = Color3.new(1, 1, 1) b.Font = Enum.Font.SourceSansBold b.TextScaled = true b.BorderSizePixel = 0 b.Parent = m tb[n] = b

local f = Instance.new("Frame") f.Position = UDim2.new(0, 0, 0, 60) f.Size = UDim2.new(1, 0, 1, -60) f.BackgroundTransparency = 1 f.Visible = false f.Parent = m tbfr[n] = f

b.MouseButton1Click:Connect(function() for k, v in pairs(tbfr) do v.Visible = (k == n) end c = n end) end

c = "Main" tbfr[c].Visible = true

local function add(tab, txt, callback) local f = tbfr[tab] if not f then return end local count = #f:GetChildren() if count >= 4 then return end local btn = Instance.new("TextButton") btn.Size = UDim2.new(1, -20, 0, 40) btn.Position = UDim2.new(0, 10, 0, 10 + count * 45) btn.BackgroundColor3 = Color3.fromRGB(50, 50, 90) btn.TextColor3 = Color3.new(1, 1, 1) btn.Font = Enum.Font.SourceSansBold btn.TextScaled = true btn.BorderSizePixel = 0 btn.Text = txt btn.Parent = f btn.MouseButton1Click:Connect(callback) end

add("Main", "üîº Infinite Jump", function() _G.ij = true u.JumpRequest:Connect(function() if _G.ij and p.Character and p.Character:FindFirstChild("Humanoid") then p.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping) end end) end)

add("Main", "üö™ Noclip", function() r.Stepped:Connect(function() if p.Character then for _, v in pairs(p.Character:GetDescendants()) do if v:IsA("BasePart") then v.CanCollide = false end end end end) end)

add("Main", "üíö Godmode", function() local h = p.Character and p.Character:FindFirstChild("Humanoid") if h then h:SetStateEnabled(Enum.HumanoidStateType.Dead, false) h.Health = math.huge end end)

add("Main", "ü™ô Auto Farm", function() _G.farm = true task.spawn(function() while _G.farm and task.wait(0.2) do for _,v in pairs(workspace:GetDescendants()) do if v:IsA("BasePart") and v.Name:lower():find("coin") then if p.Character and p.Character:FindFirstChild("HumanoidRootPart") then p.Character.HumanoidRootPart.CFrame = v.CFrame + Vector3.new(0, 3, 0) end end end end end) end)

add("Visual", "üëÄ ESP", function() for _,pl in pairs(game:GetService("Players"):GetPlayers()) do if pl ~= p then pl.CharacterAdded:Connect(function(c) local h = c:WaitForChild("Head", 5) if h and not h:FindFirstChild("ESP") then local b = Instance.new("BillboardGui", h) b.Name = "ESP" b.AlwaysOnTop = true b.Size = UDim2.new(0, 100, 0, 40) b.StudsOffset = Vector3.new(0, 2, 0) local t = Instance.new("TextLabel", b) t.Size = UDim2.new(1, 0, 1, 0) t.BackgroundTransparency = 1 t.TextColor3 = Color3.new(1, 0.4, 0.4) t.TextScaled = true t.Font = Enum.Font.SourceSansBold r.RenderStepped:Connect(function() if pl.Character and pl.Character:FindFirstChild("HumanoidRootPart") and p.Character then local d = (p.Character.HumanoidRootPart.Position - pl.Character.HumanoidRootPart.Position).Magnitude t.Text = pl.Name .. " [" .. math.floor(d) .. "m]" end end) end end) end end end)

add("Visual", "‚è± –¢–∞–π–º–µ—Ä—ã", function() for _,o in ipairs(workspace:GetDescendants()) do if o:IsA("TextLabel") or o:IsA("TextBox") then if string.find(o.Text, "%d+s") then local p0 = o:FindFirstAncestorWhichIsA("BasePart") or o:FindFirstAncestorOfClass("Model") if p0 and not p0:FindFirstChild("TimerESP") then local g = Instance.new("BillboardGui", p0) g.Name = "TimerESP" g.AlwaysOnTop = true g.Size = UDim2.new(0, 120, 0, 30) g.StudsOffset = Vector3.new(0, 4, 0) local l = Instance.new("TextLabel", g) l.Size = UDim2.new(1, 0, 1, 0) l.BackgroundTransparency = 1 l.TextColor3 = Color3.new(0.4, 1, 0.4) l.TextScaled = true l.Font = Enum.Font.SourceSansBold r.RenderStepped:Connect(function() if o.Text and o:IsDescendantOf(workspace) then l.Text = o.Text end end) end end end end end)

add("Visual", "üéØ Tracker", function() end) add("Visual", "üî¶ Highlight", function() end)

add("Misc", "‚ö° SpeedBoost", function() local h = p.Character and p.Character:FindFirstChildOfClass("Humanoid") if h then h.WalkSpeed = 80 end end)

add("Misc", "üí§ Anti-AFK", function() local vu = game:GetService("VirtualUser") p.Idled:Connect(function() vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame) wait(1) vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame) end) end)

add("Misc", "üìç –¢–ü –≤–Ω–∏–∑", function() local r0 = p.Character and p.Character:FindFirstChild("HumanoidRootPart") if r0 then r0.CFrame = CFrame.new(r0.Position.X, 15, r0.Position.Z) end end)

add("Misc", "üóë –£–¥–∞–ª–∏—Ç—å –ê–Ω—Ç–∏—á–∏—Ç", function() for _,o in ipairs(getnilinstances()) do if o:IsA("Script") and string.find(o.Name:lower(), "anti") then o:Destroy() end end end)
